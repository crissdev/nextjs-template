datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../src/lib/server/db/prisma/.generated"
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  completed Boolean
  createdAt DateTime
  updatedAt DateTime

  @@map("todos")
}
